{
  "name": "dashlane-to-print",
  "version": "0.1.2",
  "description": "Creates a printable PDF out of Dashlane data (JSON format).",
  "homepage": "https://github.com/DanielGiljam/dashlane-to-print#readme",
  "bugs": "https://github.com/DanielGiljam/dashlane-to-print/issues",
  "license": "MIT",
  "author": "Daniel Giljam (https://github.com/DanielGiljam)",
  "files": [
    "index.js",
    "header-template.html",
    "footer-template.html",
    "next.config.js",
    ".next"
  ],
  "main": "index.js",
  "bin": "index.js",
  "repository": "github:DanielGiljam/dashlane-to-print",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "tsc": "tsc",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier-eslint --write \"PWD/**/*.{js,jsx,ts,tsx,json}\"",
    "tlf": "npm run tsc && npm run lint && npm run format",
    "prepare": "npm run build"
  },
  "dependencies": {
    "chrome-launcher": "^0.13.3",
    "next": "^11.1.0",
    "puppeteer-core": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "uuid": "^8.2.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@danielgiljam/eslint-config": "^2.0.1",
    "@danielgiljam/prettierrc": "^2.0.1",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.0.5",
    "sass": "^1.26.9",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": "^12.18.0"
  }
}
